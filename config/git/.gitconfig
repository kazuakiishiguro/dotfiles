[user]
	email = kazuaki@couger.co.jp
	name = Kazuaki Ishiguro
[github]
	user = kazuakiishiguro
[credential]
	helper = osxkeychain
[color]
  status = auto
  diff = auto
  branch = auto
  interactive = auto
  grep = auto
[alias]
  cm = commit -m
  st = status --short --branch
  nl = log --graph --decorate --oneline --name-status
  tl = log --graph --date=short --pretty=format:'%C(yellow)%h %C(green)%cd %C(cyan)%d %Creset%s %Cblue[%cn]'
  alog = log --graph --all --color --pretty='%x09%h %cn%x09%s %Cred%d%Creset'
  po = push origin
  co = checkout
  br = branch
  vdiff = git difftool --tool=vimdiff
[core]
  editor = nvim
	excludesfile = /Users/kazuaki/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path =
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[commit]
	template = /Users/kazuaki/.stCommitMsg
[url "github:"]
	InsteadOf = https://github.com/
  InsteadOf = git@github.com:
